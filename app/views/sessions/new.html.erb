<header class="page-header">
<h1>Login &raquo;</h1>
</header>


<%= form_tag '/login' do %>
  <section>
  <div class="form-group">
  Email: <%= text_field_tag :email %>
  </div>
  <br>
  <div class="form-group">
  Password: <%= password_field_tag :password %>
  <div>
  </section>
  <br>
  <%= submit_tag "Submit" %>

<% end %>

